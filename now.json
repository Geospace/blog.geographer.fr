{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.+)",
      "dest": "/assets/$1"
    },
    {
      "src": "/theme/(.+)",
      "dest": "/theme/$1"
    },
    {
      "src": "/(.+)",
      "dest": "/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html",
      "status": 404
    }
  ]
}